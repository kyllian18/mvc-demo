@{
    ViewData["Title"] = "HelloWorld Index Page";
    string[] heroes = (string[])ViewData["Heroes"];
}

<h2>HelloWorld Index Page</h2>
<br/>
<p>This is the index view for HelloWorld.</p>
<p>The college is @ViewData["College"].</p>
<p>The comic universe is @ViewData["ComicUniverse"].</p>

<ul>
    @for (int i = 0; i < heroes.Length; ++i)
            {
                <li>@heroes[i]</li>
            }
</ul>

<ol>
    @for (int i = 0; i < (int)ViewData["NumTimes"]; ++i)
            {
                <li>Hello World!</li>
            }
</ol>
